{"ast":null,"code":"import{createSlice}from\"@reduxjs/toolkit\";import{fetchItems}from\"./asyncActions\";import{Status}from\"./types\";var initialState={items:[],status:Status.LOADING};var itemSlice=createSlice({name:\"item\",initialState:initialState,reducers:{setItems:function setItems(state,action){state.items=action.payload;}},extraReducers:function extraReducers(builder){builder.addCase(fetchItems.pending,function(state){state.status=Status.LOADING;state.items=[];});builder.addCase(fetchItems.fulfilled,function(state,action){state.items=action.payload;state.status=Status.SUCCESS;});builder.addCase(fetchItems.rejected,function(state){state.status=Status.ERROR;state.items=[];});}});var setItems=itemSlice.actions.setItems;export{setItems};export default itemSlice.reducer;","map":{"version":3,"names":["createSlice","fetchItems","Status","initialState","items","status","LOADING","itemSlice","name","reducers","setItems","state","action","payload","extraReducers","builder","addCase","pending","fulfilled","SUCCESS","rejected","ERROR","actions","reducer"],"sources":["/Users/mac/Documents/store/my-app/src/redux/item/slice.ts"],"sourcesContent":["import { createSlice, PayloadAction } from \"@reduxjs/toolkit\";\nimport { fetchItems } from \"./asyncActions\";\nimport { Item, ItemSliceState, Status } from \"./types\";\n\nconst initialState: ItemSliceState = {\n  items: [],\n  status: Status.LOADING,\n};\n\nconst itemSlice = createSlice({\n  name: \"item\",\n  initialState,\n  reducers: {\n    setItems(state, action: PayloadAction<Item[]>) {\n      state.items = action.payload;\n    },\n  },\n\n  extraReducers(builder) {\n    builder.addCase(fetchItems.pending, (state) => {\n      state.status = Status.LOADING;\n      state.items = [];\n    });\n    builder.addCase(fetchItems.fulfilled, (state, action) => {\n      state.items = action.payload;\n      state.status = Status.SUCCESS;\n    });\n    builder.addCase(fetchItems.rejected, (state) => {\n      state.status = Status.ERROR;\n      state.items = [];\n    });\n  },\n});\n\nexport const { setItems } = itemSlice.actions;\n\nexport default itemSlice.reducer;\n"],"mappings":"AAAA,OAASA,WAAW,KAAuB,kBAAkB,CAC7D,OAASC,UAAU,KAAQ,gBAAgB,CAC3C,OAA+BC,MAAM,KAAQ,SAAS,CAEtD,GAAM,CAAAC,YAA4B,CAAG,CACnCC,KAAK,CAAE,EAAE,CACTC,MAAM,CAAEH,MAAM,CAACI,OACjB,CAAC,CAED,GAAM,CAAAC,SAAS,CAAGP,WAAW,CAAC,CAC5BQ,IAAI,CAAE,MAAM,CACZL,YAAY,CAAZA,YAAY,CACZM,QAAQ,CAAE,CACRC,QAAQ,UAAAA,SAACC,KAAK,CAAEC,MAA6B,CAAE,CAC7CD,KAAK,CAACP,KAAK,CAAGQ,MAAM,CAACC,OAAO,CAC9B,CACF,CAAC,CAEDC,aAAa,UAAAA,cAACC,OAAO,CAAE,CACrBA,OAAO,CAACC,OAAO,CAACf,UAAU,CAACgB,OAAO,CAAE,SAACN,KAAK,CAAK,CAC7CA,KAAK,CAACN,MAAM,CAAGH,MAAM,CAACI,OAAO,CAC7BK,KAAK,CAACP,KAAK,CAAG,EAAE,CAClB,CAAC,CAAC,CACFW,OAAO,CAACC,OAAO,CAACf,UAAU,CAACiB,SAAS,CAAE,SAACP,KAAK,CAAEC,MAAM,CAAK,CACvDD,KAAK,CAACP,KAAK,CAAGQ,MAAM,CAACC,OAAO,CAC5BF,KAAK,CAACN,MAAM,CAAGH,MAAM,CAACiB,OAAO,CAC/B,CAAC,CAAC,CACFJ,OAAO,CAACC,OAAO,CAACf,UAAU,CAACmB,QAAQ,CAAE,SAACT,KAAK,CAAK,CAC9CA,KAAK,CAACN,MAAM,CAAGH,MAAM,CAACmB,KAAK,CAC3BV,KAAK,CAACP,KAAK,CAAG,EAAE,CAClB,CAAC,CAAC,CACJ,CACF,CAAC,CAAC,CAEK,GAAQ,CAAAM,QAAQ,CAAKH,SAAS,CAACe,OAAO,CAA9BZ,QAAQ,CAAuB,OAAAA,QAAA,EAE9C,cAAe,CAAAH,SAAS,CAACgB,OAAO"},"metadata":{},"sourceType":"module","externalDependencies":[]}